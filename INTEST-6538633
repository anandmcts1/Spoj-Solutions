#include<stdio.h>
#include<string.h>
 int main()
{
    int i,len;int flag=0;int b;
    char n[251];int numbers;int x;
        b=0;
       scanf("%d %d",&numbers,&x);
       for(int k=0;k<numbers;k++)
       {    
       scanf("%s",n);
        len=strlen(n);
        b=0;
           for(i=0;i<len;i++)
            {
            
              b=(b*10+(n[i]-'0'))%x;
           }
            
           if(b==0){  flag=flag+1;}
             ////note that n[i]-'0' basically converts b into 
             }
     printf("%d\n",flag);
    return 0;  
     } 
